<package>
  <name>nao_teleop</name>
  <version>0.1.1</version>
  <description>
    Teleoperation (gamepad or joystick) for the Nao humanoid
  </description>
  <maintainer email="HornungA@informatik.uni-freiburg.de">Armin Hornung</maintainer>
  <license>BSD</license>

  <url>http://ros.org/wiki/nao_remote</url>
  <author>Armin Hornung</author>

  
  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>roscpp</build_depend>
  <build_depend>std_msgs</build_depend>
  <build_depend>std_srvs</build_depend>
  <build_depend>nao_msgs</build_depend>
  <build_depend>sensor_msgs</build_depend>
  <build_depend>actionlib</build_depend>

  
  <!--Same as build dependencies -->
  <run_depend>roscpp</run_depend>
  <run_depend>std_msgs</run_depend>
  <run_depend>std_srvs</run_depend>
  <run_depend>nao_msgs</run_depend>
  <run_depend>sensor_msgs</run_depend>
  <run_depend>actionlib</run_depend>
  
  <!-- New build dependencies (for launch files) -->
  <run_depend>nao_remote</run_depend>
  <run_depend>joy</run_depend>

</package>
